package com.desafio.crudpessoa.controllers;

import com.desafio.crudpessoa.entities.Pessoa;

public class ResponseObject {

	private String message;
	private Pessoa content;
	
	public ResponseObject(String message, Pessoa content) {
		this.message = message;
		this.content = content;
	}
	
	public String getMessage() {
		return message;
	}
	public void setMessage(String message) {
		this.message = message;
	}
	public Pessoa getContent() {
		return content;
	}
	public void setContent(Pessoa content) {
		this.content = content;
	}
	
}
